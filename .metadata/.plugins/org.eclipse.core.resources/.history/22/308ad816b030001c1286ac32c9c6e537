</select>

<insert id="insertAjaxReply" parameterType="sist.com.vo.ReplyVo">
 INSERT INTO REPLYDEMO(NO, ID, REGDATE, STEP, LEV, PNUM, REF, REPLY)
 VALUES(#{no},#{id},SYSDATE,#{step},#{lev},#{pnum},#{ref},0)
</insert>

